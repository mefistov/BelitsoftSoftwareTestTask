namespace BelitsoftSoftwareTestTask.Models
{
    public class GetSearchCruisesResponce<Ships>
    {
        public int TotalPages { get; set; };
        public int TotalResults { get; set; };
        public List<Ships> List { get; set; };
    }
    public GetSearchCruisesResponce(int totalPages, int totalResults, Ships ships)
        {
            TotalPages = totalPages;
            TotalResults = totalResults;
            list = ships;
        }
}